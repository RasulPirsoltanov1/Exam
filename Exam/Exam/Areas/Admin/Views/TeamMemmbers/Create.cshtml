

@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
@model TeamMemmberDTO
<div class="card-body">
    <h4 class="card-title">Create Team Memmber</h4>
    <p class="card-description">
        Basic form elements
    </p>
    <form class="forms-sample" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label for="exampleInputName1">Name</label>
            <input  asp-for=Name class="form-control" id="exampleInputName1" placeholder="Name">
        </div>

        <div class="form-group">
            <label for="exampleInputName1">Position</label>
            <input   asp-for=Position class="form-control" id="exampleInputName1" placeholder="Name">
        </div>
        <div class="form-group">
            <label for="exampleInputName1">Facebook</label>
            <input   asp-for=Facebook class="form-control" id="exampleInputName1" placeholder="Name">
        </div>
          <div class="form-group">
      <label for="exampleInputName1">Twitter</label>
            <input   asp-for=Twitter class="form-control" id="exampleInputName1" placeholder="Name">
        </div>
        <div class="form-group">
            <label for="exampleInputName1">Instagram</label>
            <input   asp-for=Instagram class="form-control" id="exampleInputName1" placeholder="Name">
        </div>
        <div class="form-group">
            <label for="exampleInputName1">Linkedin</label>
            <input   asp-for=Linkedin class="form-control" id="exampleInputName1" placeholder="Name">
        </div>
      
        <div class="form-group">
            <label>Photo upload</label>
            <input type="file" asp-for=Photo class="file-upload-default">
            <div class="input-group col-xs-12">
                <input type="file" class="form-control file-upload-info"  placeholder="Upload Image">
                <span class="input-group-append">
                    <button class="file-upload-browse btn btn-primary" type="button">Upload</button>
                </span>
            </div>
        </div>
    
        <div class="form-group">
            <label for="exampleTextarea1">About</label>
            <textarea class="form-control" asp-for=About id="exampleTextarea1" rows="4"></textarea>
        </div>
        <button type="submit" class="btn btn-primary mr-2">Submit</button>
        <button class="btn btn-light">Cancel</button>
    </form>
</div>

